        If flag_HCD_10T_NL = &H0 Then write_HCD_10T_NL_value = get_HCD_10T_NL

        regValue = leftShift((write_ENABLE_SHADOW_REGISTER_value and &H1), 15) + leftShift((write_GB_ADV_DIS_value and &H1), 14) + leftShift((write_TX_ADV_DIS_value and &H1), 13) + leftShift((write_WIRESPEED_DOWNGRADE_value and &H1), 12) + leftShift((write_HCD_1000T_FDX_value and &H1), 11) + leftShift((write_HCD_1000T_value and &H1), 10) + leftShift((write_HCD_100T_FDX_value and &H1), 9) + leftShift((write_HCD_100T_value and &H1), 8) + leftShift((write_HCD_10T_FDX_value and &H1), 7) + leftShift((write_HCD_10T_value and &H1), 6) + leftShift((write_HCD_1000T_FDX_NL_value and &H1), 5) + leftShift((write_HCD_1000T_NL_value and &H1), 4) + leftShift((write_HCD_100T_FDX_NL_value and &H1), 3) + leftShift((write_HCD_100T_NL_value and &H1), 2) + leftShift((write_HCD_10T_FDX_NL_value and &H1), 1) + leftShift((write_HCD_10T_NL_value and &H1), 0)
